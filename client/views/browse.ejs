                <option value="Desc">Descending</option>
              </select>
          </div>
          <!-- Default input -->
          <div class="form-group col-md-6">
            <label for="gender">Tags</label>
            <select class="browser-default custom-select" name="tags-sort">
              <option value="Asc">Ascending</option>
                <option value="Desc">Descending</option>
              </select>
          </div>
          <button type="button" class="btn btn-primary btn-lg btn-block" style="margin: 15;">Sort</button>

          </div>
      </div>
      
    </div>
  </div>
