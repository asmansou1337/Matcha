<section class="view">
  <!--Accordion wrapper-->
<div class="accordion md-accordion" id="accordionEx" role="tablist" aria-multiselectable="true">

  <!-- Accordion card -->
  <div class="card">

    <!-- Card header -->
    <div class="card-header" role="tab" id="headingOne1">
      <a data-toggle="collapse" data-parent="#accordionEx" href="#collapseOne1" aria-expanded="true"
        aria-controls="collapseOne1">
        <h5 class="mb-0">
          Filter <i class="fas fa-angle-down rotate-icon"></i>
        </h5>
      </a>
    </div>

    <!-- Card body -->
    <div id="collapseOne1" class="collapse show" role="tabpanel" aria-labelledby="headingOne1" data-parent="#accordionEx">
      <div class="card-body">
        <div class="row" style="margin: 10;">
          <div class="col-sm-4">
              <!-- Ranger Slider with value -->
              <!-- Slider -->
              <div class="text-center"><span>Age (18 to 120)</span></div>
              <div id="pmd-slider-age"  class="pmd-range-slider"></div>	
              <!-- Values -->										
              <div class="row">
                  <div class="range-value col-sm-6">
                      <span id="age-min"></span>
                  </div>
                  <div class="range-value col-sm-6 text-right">
                      <span id="age-max"></span>
                  </div>
              </div>
          </div>
          <div class="col-sm-4">
              <!-- Ranger Slider with value -->
              <!-- Slider -->
              <div class="text-center"><span>Rating (0 to 5)</span></div>
              <div id="pmd-slider-rating"  class="pmd-range-slider"></div>	
              <!-- Values -->										
              <div class="row">
                  <div class="range-value col-sm-6">
                      <span id="rating-min"></span>
                  </div>
                  <div class="range-value col-sm-6 text-right">
                      <span id="rating-max"></span>
                  </div>
              </div>
          </div>
          <div class="col-sm-4">
              <!-- Ranger Slider with value -->
              <!-- Slider -->
              <div class="text-center"><span>Location (0 to 200km)</span></div>
              <div id="pmd-slider-location"  class="pmd-range-slider"></div>	
              <!-- Values -->										
              <div class="row">
                  <div class="range-value col-sm-6">
                      <span id="location-min"></span>
                  </div>
                  <div class="range-value col-sm-6 text-right">
                      <span id="location-max"></span>
                  </div>
              </div>
          </div>    
        </div>
        <div>
            <!-- tags selection -->
            <div class="row justify-content-center">
              <div class="col-sm-8">
              <div class="text-center"><span>Tags</span></div>
              <div class="container-fluid border rounded mb-0 tag-container autocomplete" style="padding: 10px;">
                  <input placeholder ="Select tags..." id="myInput"/>
              </div>
            </div>
          </div>
            <!-- end tags selection -->
            <div class="row justify-content-center">
              <button type="button" class="btn btn-primary btn-lg btn-block" style="margin: 15;">Filter</button>
            </div>
        </div>
      </div>
    </div>

  </div>
  <!-- Accordion card -->

